  <ThingDef ParentName="XennArmorMakeableBase">
    <defName>Xenn_Armored_DusterXX</defName>
    <label>Xenn armor with duster</label>
    <description>A suit of power armor with a large camo duster over it. Standard issue for most Xenn terrestrial operations.</description>
    <techLevel>Spacer</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>PoweredArmor</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>XennArmor/DustArmor/ArmorDuster</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>170000</WorkToMake>
      <MaxHitPoints>450</MaxHitPoints>
      <MarketValue>4700</MarketValue>
      <Mass>15</Mass>
      <ArmorRating_Blunt>0.46</ArmorRating_Blunt>
      <ArmorRating_Sharp>1.16</ArmorRating_Sharp>
      <ArmorRating_Heat>0.61</ArmorRating_Heat>
      <Insulation_Cold>36</Insulation_Cold>
      <Insulation_Heat>25</Insulation_Heat>
    </statBases>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <costList>
      <ComponentIndustrial>18</ComponentIndustrial>
      <Plasteel>135</Plasteel>
      <Steel>35</Steel>
      <Cloth>15</Cloth>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>XennArmor/DustArmor/ArmorDuster</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
      <tags>
        <li>XennSoldier</li>
      </tags>
    </apparel>
  </ThingDef>
